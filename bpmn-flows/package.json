{
  "name": "@nihasoft/bpmn-flows",
  "version": "0.1.3",
  "description": "Mechanism to view and edit bpmn 2.0 in angular",
  "scripts": {
    "copy": "cpx src/lib/bpmn-flows.component.scss dist/bpmn-flows/ && cpx src/lib/bpmn-flows.variables.scss dist/bpmn-flows/ && cpx ../README.md dist/bpmn-flows",
    "copy-dist": "cpx dist/ ../bpmn-flows-demo/node_modules/@nihasoft/",
    "build-prod": "ng build --prod && npm run copy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish-prod": "npm run build-prod && cd dist/bpmn-flows && npm publish --access=public",
    "publish-dev": "npm run build-prod && npm run copy-dist",
    "publish-beta": "npm run build-prod && cd dist/bpmn-flows && npm publish --access=public --tag beta"
  },
  "peerDependencies": {
    "@angular/animations": "^7.2.6",
    "@angular/common": "^7.2.6",
    "@angular/compiler": "^7.2.6",
    "@angular/core": "^7.2.6",
    "zone.js": "~0.8.26"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "d3": "^5.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "^0.13.3",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "^7.2.6",
    "cpx": "^1.5.0",
    "ng-packagr": "^4.7.1",
    "tsickle": "^0.34.3",
    "typescript": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nihasoft/bpmn-flows.git"
  },
  "bugs": {
    "url": "https://github.com/nihasoft/bpmn-flows/issues"
  },
  "donations": "https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=96LEHU7G2Z2MC",
  "keywords": [
    "angular bpmn",
    "bpmn",
    "camunda bpmn",
    "camunda angular",
    "bpmn typescript",
    "bpmn 2.0",
    "angular 6 bpmn",
    "angular 6 bpmn 2.0",
    "bpmn angular",
    "bpmn camunda",
    "angular camunda bpmn",
    "camunda angular bpmn",
    "bpmn camunda angular",
    "bpmn-flows",
    "bpmn flows",
    "angular bpmn-flows",
    "flows bpmn",
    "@nihasoft"
  ],
  "author": "Hanor SÃ¡tiro Cintra <hanor.cintra@outlook.com>",
  "license": "MIT"
}
